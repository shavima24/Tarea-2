#include <stdio.h>

int main() 
{
    int num1, num2;
    int suma, resta, multiplicacion;

    printf("Ingresa dos números: ");
    scanf("%d %d", &num1, &num2);

    suma = num1 + num2;
    resta = num1 - num2;
    multiplicacion = num1 * num2;

    printf("Suma: %d\n", suma);
    printf("Resta: %d\n", resta);
    printf("Multiplicación: %d\n", multiplicacion);

    return 0;
}
