#include <stdio.h>

int main() 
{
    int N, i = 1;
    unsigned long long producto = 1;

    printf("Ingrese el valor de N: ");
    scanf("%d", &N);

    while (i <= N) 
    {
        producto *= i;
        i++;
    }

    printf("El producto de los primeros %d nÃºmeros naturales es: %llu\n", N, producto);

    return 0;
}
