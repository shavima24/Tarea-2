//Ejercicio 16

#include <stdio.h>

int main() 
{
    int numeros_pares[100]; 
    int i;

    for (i = 0; i < 100; i++) 
    {
        numeros_pares[i] = (i + 1) * 2;
    }

    printf("Los primeros 100 nÃºmeros pares son:\n");
    for (i = 0; i < 100; i++) 
    {
        printf("%d ", numeros_pares[i]);
    }
    
    return 0;
}
